        <Button Content="3号车间" HorizontalAlignment="Left" Margin="333,72,0,0" VerticalAlignment="Top" Width="92" Height="47" FontSize="18" Background="{x:Null}" FontWeight="Bold"/>
        <Button Content="2号车间" HorizontalAlignment="Left" Margin="181,74,0,0" VerticalAlignment="Top" Width="92" Height="47" Background="{x:Null}" FontSize="18" FontWeight="Bold"/>
        <Button Content="4号车间" HorizontalAlignment="Left" Margin="472,76,0,0" VerticalAlignment="Top" Width="92" Height="47" FontSize="18" FontWeight="Bold" Background="{x:Null}"/>
        <Button Content="5号车间" HorizontalAlignment="Left" Margin="624,76,0,0" VerticalAlignment="Top" Width="92" Height="47" Background="{x:Null}" FontSize="18" FontWeight="Bold"/>
        <Button Content="6号车间" HorizontalAlignment="Left" Margin="28,165,0,0" VerticalAlignment="Top" Width="92" Height="47" FontSize="18" FontWeight="Bold" Background="{x:Null}"/>
        <Button Content="7号车间" HorizontalAlignment="Left" Margin="181,168,0,0" VerticalAlignment="Top" Width="92" Height="47" FontSize="18" FontWeight="Bold" Background="{x:Null}"/>
        <Label Content="正在建造" HorizontalAlignment="Left" Margin="28,217,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold"/>
        <Label Content="正在建造" HorizontalAlignment="Left" Margin="181,220,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold"/>
        <Label Content="等待加入........" HorizontalAlignment="Left" Margin="333,181,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold"/>
        <Button Content="返回主页面" HorizontalAlignment="Left" Margin="28,287,0,0" VerticalAlignment="Top" Width="123" Height="58" FontSize="20" FontWeight="Bold" Background="{x:Null}" Click="Button_Click"/>

    </Grid>
